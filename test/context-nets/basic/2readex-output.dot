digraph PetriNet {
// Places
p3 [shape=circle label="c3 (s0')"];
p5 [shape=circle label="c5 (s')"];
p4 [shape=circle label="c4 (s1')"];
p1 [shape=circle label="c1 (s0)"];
p2 [shape=circle label="c2 (s)"];
// Transitions
t4 [shape=box label="e4 (t)"];
t3 [shape=box label="e3 (t1)"];
t2 [shape=box label="e2 (t0)"];
// Edges
p1 -> t3;
p1 -> t2;
p2 -> t4;
t4 -> p5;
t3 -> p4;
p2 -> t3 [dir=none];
t2 -> p3;
p2 -> t2 [dir=none];
}
